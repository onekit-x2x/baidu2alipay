<view  class="container">
    <view  class="page-body">
        <swan-button  onTap="showNavigationBarLoading" type="primary" hover-stop-propagation="true">显示加载动画</swan-button>
        <swan-button  onTap="hideNavigationBarLoading" type="primary" hover-stop-propagation="true">隐藏加载动画</swan-button>
    </view>
    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>