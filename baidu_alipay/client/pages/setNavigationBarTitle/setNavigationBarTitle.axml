<view  class="container">
    <view  class="page-body">
        <input  onInput="titleInput" class="input" type="text" placeholder="请输入页面标题并点击设置即可"></input>
        <swan-button  onTap="setNavigationBarTitle" type="primary" hover-stop-propagation="true">设置</swan-button>
    </view>
    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>