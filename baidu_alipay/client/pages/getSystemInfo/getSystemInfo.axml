<view  class="container">
    <view  class="page-body">
        <view  class="info-ctn">
            <view  class="info-item" a:for-item="item" a:key="item" a:for="{{infoList}}">
                <text  class="info-label">{{item.label}}：</text>
                <text  class="info-value">{{item.value}}</text>
            </view>
        </view>
        <view  class="btn-ctn">
            <swan-button  onTap="getSystemInfo" type="primary" hover-stop-propagation="true">点击获取系统信息</swan-button>
            <swan-button  onTap="clearClick" hover-stop-propagation="true">清空</swan-button>
        </view>
    </view>
    <view  class="page-title">
        <view  class="page-title-line"></view>
        <view  class="page-title-text">{{title}}</view>
    </view>
</view>