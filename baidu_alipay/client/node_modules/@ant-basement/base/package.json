{
  "name": "@ant-basement/base",
  "author": "SangLv <sanglv.wl@antfin.com>",
  "version": "1.0.2",
  "description": "Service Clients for Basement SDK",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/lib"
  ],
  "homepage": "http://gitlab.alipay-inc.com/basement-cloud/bmc-base-sdk",
  "repository": {
    "type": "git",
    "url": "git@gitlab.alipay-inc.com:basement-cloud/bmc-base-sdk.git"
  },
  "bugs": {
    "url": "http://gitlab.alipay-inc.com/basement-cloud/bmc-base-sdk/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ci": {
    "versions": "8, 10"
  },
  "scripts": {
    "autod": "autod",
    "tsc": "rimraf dist && tsc -p tsconfig.json",
    "tsc:w": "npm run tsc -- -w",
