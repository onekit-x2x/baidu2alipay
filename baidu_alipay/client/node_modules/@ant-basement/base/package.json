    "@types/node": "^10.0.3",
    "ali-contributors": "^2.1.1",
    "autod": "3",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@ant-basement/errors": "^1.0.0",
