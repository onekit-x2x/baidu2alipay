    "ci": "npm run lint && npm run cov",
    "prepare": "npm run tsc && npm run ci"
  },
  "devDependencies": {
    "@ali/ci": "^3.12.2",
    "@ali/egg-bin": "^2.5.0",
    "@ali/mm": "6",
